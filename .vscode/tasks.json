{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc-9 build active file",
			"command": "gcc",
			"args": [
				/*"-Wall",
				"-Werror",
				"-Wextra",
				"-pedantic",*/
				"*.c",
				"-o",
				"baby_shell"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /bin/gcc-9"
		}
	]
}